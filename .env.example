DD_BOT_ENABLE=false
DD_BOT_TOKEN=你的钉钉机器人Token
DD_BOT_SECRET=你的钉钉机器人密钥

# Telegram通知配置（可选）
TG_BOT_ENABLE=false
TG_BOT_TOKEN=你的Bot Token
TG_CHAT_ID=你的Chat ID
